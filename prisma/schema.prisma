generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int     @id @default(autoincrement())
  discordId Int     @unique
  username  String
  Score     Score[]
}

model Score {
  id        Int      @id @default(autoincrement())
  createdAt DateTime @default(now())
  user      User     @relation(fields: [discordId], references: [discordId])
  discordId Int
  scorename String
  score     Int
}
